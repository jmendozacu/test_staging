/* custom css */

	@font-face {
	  font-family: 'Raleway';
	  font-weight: 400;
	  font-style: normal;
	  src: url('fonts/Raleway-regular.eot');
	  src: url('fonts/Raleway-regular.eot?#iefix') format('embedded-opentype'),
	       local('Raleway'),
	       local('Raleway-regular'),
	       url('fonts/Raleway-regular.woff2') format('woff2'),
	       url('fonts/Raleway-regular.woff') format('woff'),
	       url('fonts/Raleway-regular.ttf') format('truetype'),
	       url('fonts/Raleway-regular.svg#Raleway') format('svg');
	}

	@font-face {
	  font-family: 'Raleway';
	  font-weight: 700;
	  font-style: normal;
	  src: url('fonts/Raleway-700.eot');
	  src: url('fonts/Raleway-700.eot?#iefix') format('embedded-opentype'),
	       local('Raleway Bold'),
	       local('Raleway-700'),
	       url('fonts/Raleway-700.woff2') format('woff2'),
	       url('fonts/Raleway-700.woff') format('woff'),
	       url('fonts/Raleway-700.ttf') format('truetype'),
	       url('fonts/Raleway-700.svg#Raleway') format('svg');
	}

	body {
		font-family: 'Raleway', sans-serif;
	}

	body.boxed .wrapper {
		padding: 0;
	}

