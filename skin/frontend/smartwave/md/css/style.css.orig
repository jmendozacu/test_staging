.header-wrapper {
  margin-top: 10px;
}
.header .logo {
  margin-bottom: 10px;
}
.header .logo img {
  height: auto;
  max-width: 270px;
}
.header .form-search #search,
.header-container.type3 .header .form-search {
  border-radius: 3px;
  color: #000000;
  height: auto;
  padding: 3px 8px;
}
.header-container.type3 .header .form-search #search {
  color: #000000;
}
.header-container.type3 .header .form-search {
  background: #e6f0f5;
}
.header-container.type3 .header .form-search #cat {
  background: #e40305;
  border: #e40305;
}
.header-container.type3 .header .form-search #cat:focus {
  outline: none;
}
.header-container.type3 .header .form-search button.button {
  background: rgba(0, 0, 0, 0) url("../img/suche.png") no-repeat scroll 0 0;
  border-radius: 0 3px 3px 0;
  margin: 2px;
  width: 36px;
  height: 28px;
}
.header-container.type3 .header #search_mini_form {
  background: #e6f0f5;
  border-radius: 3px;
  margin-top: 10px;
  border: 1px solid #d9d9d9;
  border-top-color: #c0c0c0;
  -webkit-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.1) inset;
  -moz-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.1) inset;
  box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.1) inset;
}
.header-container.type3 .top-links-container {
  background-color: transparent;
}
.header-container.type3 .top-links-container li a::before {
  background: rgba(0, 0, 0, 0) url("../img/li.png") no-repeat scroll 0 0;
  content: "";
  display: inline-block;
  height: 7px;
  width: 10px;
}
.header-container.type3 .top-links-container a,
.header-container.type3 .top-links ul.links a {
  font-size: 12px;
  line-height: 18px;
  text-transform: none;
  font-weight: bold;
  color: #000000;
}
.header-container.type3 .links li {
  border: none;
  padding-left: 10px;
  padding-right: 10px;
  margin: 0;
}
.header-leistungen li {
  float: left;
  margin-right: 40px;
}
.header-leistungen li span {
  color: #85888a;
  float: left;
  margin-top: 5px;
  display: block;
}
.header-leistungen li img {
  float: left;
  margin-right: 10px;
}
.header-icon {
  margin-top: 10px;
  margin-left: 10px;
  margin-right: 10px;
}
.mini-cart .cart-qty {
  display: block;
  font-size: 16px;
  height: 30px;
  padding-top: 4px;
  width: 30px;
  border: 3px solid #ffffff;
  border-radius: 100%;
  background: #e71f00;
  /* Old browsers */
  background: -moz-linear-gradient(top, #e71f00 0%, #b31600 83%, #b31600 100%);
  /* FF3.6-15 */
  background: -webkit-linear-gradient(top, #e71f00 0%, #b31600 83%, #b31600 100%);
  /* Chrome10-25,Safari5.1-6 */
  background: linear-gradient(to bottom, #e71f00 0%, #b31600 83%, #b31600 100%);
  /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#e71f00', endColorstr='#b31600', GradientType=0);
  /* IE6-9 */
}
.mini-cart .mybag-link {
  background: rgba(0, 0, 0, 0) url("../img/cart.png") no-repeat scroll 0 0;
  height: 40px;
  width: 40px;
}
.header-container.type3 .mybag-link .cart-info {
  margin-left: 20px;
  margin-top: -35px;
  position: absolute;
}
.mini-cart-total {
  display: block;
  height: 40px;
  width: 100%;
  position: absolute;
  right: 0;
}
.mini-cart-total .price {
  color: #000;
  display: block;
  height: 50px;
  width: 100%;
  font-size: 18px;
  font-family: "dosisbold", Helvetica, Arial, sans-serif;
}
.header-container.type3 .mini-cart {
  left: auto;
  right: 60px;
  top: 25px;
}
.cart-area {
  left: auto;
  right: 0;
}
.topCartContent {
  border-radius: 0;
  border-color: #e40305;
}
.topCartContent::before {
  border-color: transparent transparent #e40305;
}
.main-nav {
  margin: 0 auto;
  max-width: 1150px;
  background: #e71f00;
  /* Old browsers */
  background: -moz-linear-gradient(top, #e71f00 0%, #b31600 83%, #b31600 100%);
  /* FF3.6-15 */
  background: -webkit-linear-gradient(top, #e71f00 0%, #b31600 83%, #b31600 100%);
  /* Chrome10-25,Safari5.1-6 */
  background: linear-gradient(to bottom, #e71f00 0%, #b31600 83%, #b31600 100%);
  /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#e71f00', endColorstr='#b31600', GradientType=0);
  /* IE6-9 */
}
.footer {
  background-color: #ffffff;
}
.footer-top,
.footer-payments,
.footer-menu {
  max-width: 1180px;
  margin-left: auto;
  margin-right: auto;
  padding-left: 15px;
  padding-right: 15px;
}
.footer h3 {
  border-bottom: 1px solid #e6f0f5;
  color: #000000;
  font-size: 15px;
  line-height: 22px;
  padding-bottom: 5px;
}
.footer-text-wrapper {
  max-width: 500px;
}
.footer-menu ul li {
  line-height: 22px;
}
.footer-menu ul li a::before {
  background: rgba(0, 0, 0, 0) url("../img/li.png") no-repeat scroll 0 0;
  content: "";
  display: inline-block;
  height: 7px;
  width: 10px;
  margin-right: 10px;
}
.footer .icon {
  margin-right: 10px;
  margin-bottom: 10px;
}
.footer-newsletter {
  background: #e71f00;
  /* Old browsers */
  background: -moz-linear-gradient(top, #e71f00 0%, #b31600 83%, #b31600 100%);
  /* FF3.6-15 */
  background: -webkit-linear-gradient(top, #e71f00 0%, #b31600 83%, #b31600 100%);
  /* Chrome10-25,Safari5.1-6 */
  background: linear-gradient(to bottom, #e71f00 0%, #b31600 83%, #b31600 100%);
  /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#e71f00', endColorstr='#b31600', GradientType=0);
